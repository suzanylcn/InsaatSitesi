@using InsaatSite.Models
@model InsaatSite.Models.proje
@{
	ViewBag.Title = "Projedetay";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="ftco-section">
	<div class="container">
		<div class="row">
			<div class="col-lg-8 ftco-animate">
				<h2 class="mb-3">@Model.ad</h2>
				<p>
					<img src="@Url.Content("~/resimler/proje")/@Model.resim" class="img-fluid">
				</p>

				<p>@Model.aciklama</p>
			</div>
		</div>
	</div>
</section>

